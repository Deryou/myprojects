                    <RadioButton
                        android:id="@+id/bdbj_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_18"
                        android:checked="true"
                        android:text="符合"/>
                    <RadioButton
