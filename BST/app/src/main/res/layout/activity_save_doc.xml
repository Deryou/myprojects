                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_18"
                        android:text="不符合"/>
                </RadioGroup>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/dp_10">
