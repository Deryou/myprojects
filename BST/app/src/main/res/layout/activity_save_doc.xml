                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:imeOptions="actionNext"
                    android:background="@drawable/white_background_oval_shape"
                    android:layout_marginLeft="@dimen/dp_20"/>
            </LinearLayout>

            <LinearLayout
