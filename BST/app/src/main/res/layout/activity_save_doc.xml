                    android:layout_alignParentRight="true">
                    <RadioButton
                        android:id="@+id/qcbj_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_18"
                        android:checked="true"
                        android:text="符合"/>
                    <RadioButton
                        android:id="@+id/qcbj_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_18"
                        android:text="不符合"/>
                </RadioGroup>
